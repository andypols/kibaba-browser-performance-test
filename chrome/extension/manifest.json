{
  "manifest_version": 2,
  "name": "Kibana system pump (beat)",
  "version": "0.0.1",
  "description": "Send system status like CPU, memory to Kibana",
  "homepage_url": "https://github.com/andypols/kibana-browser-performance-test",
  "offline_enabled": true,
  "background": {
    "scripts": [
      "dist/background.js"
    ]
  },
  "permissions": [
    "system.cpu",
    "system.memory",
    "storage",
    "debugger",
    "http://localhost/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Web Page Performance Monitor"
  },
  "options_ui": {
    "page": "dist/options.html",
    "chrome_style": true
  },
  "icons": {
    "128": "icon.png"
  }
}
